{
    "name": "projectname",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "android-build:local": "cd android && ./gradlew assembleLocal",
        "android-build:prod": "cd android && ./gradlew assembleProduction",
        "android-build:staging": "cd android && ./gradlew assembleStaging",
        "android:local-release": "react-native run-android --variant=localrelease --appIdSuffix=local",
        "android:local": "react-native run-android --variant=developmentdebug --appIdSuffix=local",
        "android:prod-release": "react-native run-android --variant=productionrelease",
        "android:prod": "react-native run-android --variant=productiondebug",
        "android:staging-release": "react-native run-android --variant=stagingrelease --appIdSuffix=staging",
        "android:staging": "react-native run-android --variant=stagingdebug --appIdSuffix=staging",
        "build:tailwind": "tailwindcss --input input.css --output tailwind.css --no-autoprefixer && tailwind-rn",
        "dev:tailwind": "concurrently \"tailwindcss --input input.css --output tailwind.css --no-autoprefixer --watch\" \"tailwind-rn --watch\"",
        "ios": "react-native run-ios",
        "lint": "eslint .",
        "pod": "npx pod-install",
        "postinstall": "npx patch-package",
        "prettier:check": "npx prettier --check 'src/**/*.{js,ts,jsx,tsx,json}'",
        "prettier": "npx prettier --write 'src/**/*.{js,ts,jsx,tsx,json}'",
        "start": "react-native start",
        "test": "jest",
        "typecheck": "tsc --noemit --skipLibCheck"
    },
    "dependencies": {
        "@react-navigation/bottom-tabs": "^6.5.8",
        "@react-navigation/native": "^6.1.7",
        "@react-navigation/native-stack": "^6.9.13",
        "@reduxjs/toolkit": "^1.9.5",
        "expo": "^48.0.0",
        "i18next": "^23.2.3",
        "react": "18.2.0",
        "react-i18next": "^13.0.1",
        "react-native": "0.71.11",
        "react-native-config": "^1.5.1",
        "react-native-safe-area-context": "^4.6.3",
        "react-native-screens": "^3.22.0",
        "react-native-svg": "^13.9.0",
        "react-redux": "^8.1.1",
        "tailwind-rn": "^4.2.0"
    },
    "devDependencies": {
        "@babel/core": "^7.20.0",
        "@babel/eslint-parser": "^7.22.5",
        "@babel/preset-env": "^7.20.0",
        "@babel/runtime": "^7.20.0",
        "@ianvs/prettier-plugin-sort-imports": "^4.0.2",
        "@react-native-community/eslint-config": "^3.2.0",
        "@tsconfig/react-native": "^2.0.2",
        "@types/jest": "^29.2.1",
        "@types/react": "^18.0.24",
        "@types/react-test-renderer": "^18.0.0",
        "@typescript-eslint/eslint-plugin": "^5.60.1",
        "@typescript-eslint/parser": "^5.60.1",
        "babel-jest": "^29.2.1",
        "concurrently": "^8.2.0",
        "eslint": "^8.19.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "jest": "^29.2.1",
        "metro-config": "^0.76.0",
        "postcss": "^8.4.24",
        "prettier": "^2.4.1",
        "react-native-svg-transformer": "^1.0.0",
        "react-test-renderer": "18.2.0",
        "tailwindcss": "^3.3.2",
        "typescript": "4.8.4"
    },
    "jest": {
        "preset": "react-native"
    }
}
